 "font_size": 14,
 "html": "<style>\r\n    .font_size>tbody>tr{\r\n        font-size: 12px !important;\r\n    }\r\n</style>\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {%- if letter_head and not no_letterhead -%}\r\n    <div class=\"letter-head\" >{{ letter_head }}</div>\r\n    {%- else -%}\r\n    <div>\r\n        <br>\r\n    </div>\r\n    {%- endif -%}\r\n</div>\r\n{% if doc.status == 'Completed' %}\r\n{% if frappe.db.get_value(\"Stock Entry\",{'work_order':doc.name,'docstatus':1, 'purpose':\"Manufacture\"},'name') %}\r\n{% set se_name = frappe.get_doc(\"Stock Entry\",{'work_order':doc.name,'docstatus':1, 'purpose':\"Manufacture\"}) %}\r\n\r\n<h1 align=\"center\" style=\"margin-top: 20px\">\r\n        <strong>\r\n            Manufactured: \r\n            {% for row in se_name.items %}\r\n            {% if row.t_warehouse %}\r\n                {{ row.item_code }}\r\n                    {% if loop.index != loop.length %},\r\n                    {% endif %}\r\n                {% endif %}\r\n            {% endfor %}\r\n    </strong>\r\n</h1>\r\n{% endif %}\r\n<table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" style=\"margin-top: 20px\" class=\"font_size\">\r\n    <tbody>\r\n        <tr>\r\n            <th colspan=\"8\" align=\"center\"><h5 align=\"center\"><b>Raw Material Costs</b></h5></th>\r\n        </tr>\r\n        <tr>\r\n            <th width=\"6%\" valign=\"top\">\r\n                <strong>No</strong>\r\n            </th>\r\n            <th width=\"25%\" valign=\"top\">\r\n                <strong> Raw Material</strong>\r\n            </th>\r\n            <th width=\"8%\" valign=\"top\">\r\n                <strong>Lot No</strong>\r\n            </th>\r\n            <!--<th width=\"8%\" valign=\"top\">-->\r\n            <!--    <strong>Purity</strong>-->\r\n            <!--</th>-->\r\n            <th width=\"8%\" valign=\"top\">\r\n                <strong> Quantity</strong>\r\n            </th>\r\n            <th width=\"10%\" valign=\"top\">\r\n                <strong>Rate</strong>\r\n            </th>\r\n            <th width=\"18%\" valign=\"top\">\r\n                <strong>Amount</strong>\r\n            </th>\r\n            <!--<th width=\"16%\" valign=\"top\">-->\r\n            <!--    <strong>Per Unit Effect</strong>-->\r\n            <!--</th>-->\r\n        </tr>\r\n\r\n        {% if frappe.db.get_value(\"Stock Entry\",{'work_order':doc.name,'docstatus':1, 'purpose':\"Manufacture\"},'name') %}\r\n        {% set se = frappe.get_doc(\"Stock Entry\",{'work_order':doc.name,'docstatus':1, 'purpose':\"Manufacture\"}) %}\r\n        {% set real_sum = 0 %}\r\n        {% set count = namespace(idx=1) %}\r\n        {% set item = namespace(code='0') %}\r\n        {% for i in doc.required_items %}\r\n            {% set item.code = i.item_code %}\r\n            {% for row in se.items %}\r\n                {% if not row.t_warehouse %}\r\n                    {% if row.item_code == item.code %}\r\n                        <tr>\r\n                            <td width=\"6%\" valign=\"top\">\r\n                               {{ count.idx }}\r\n                                {% set count.idx = count.idx+1 %}\r\n                            </td>\r\n                            <td width=\"25%\" valign=\"top\">\r\n                                {{ row.item_code }}\r\n                            </td>\r\n                            <td width=\"8%\" align=\"right\" valign=\"top\">\r\n                                {{ row.lot_no or \"-\" }}\r\n                            </td>\r\n                            <!--<td width=\"8%\" align=\"right\" valign=\"top\">-->\r\n                            <!--    {{ row.concentration or \"-\" }}-->\r\n                            <!--</td>-->\r\n                            <td width=\"8%\" valign=\"top\" align=\"right\">\r\n                                {{ row.quantity|flt(2) or row.qty|flt(2) }}\r\n                            </td>\r\n                            <td width=\"10%\" valign=\"top\" align=\"right\">\r\n                                {{ row.get_formatted('price') or row.get_formatted('valuation_rate') }}\r\n\r\n                            </td>\r\n                            <td width=\"18%\" valign=\"top\" align=\"right\">\r\n                                {% set amnt = (row.qty* row.valuation_rate)  %}\r\n                                {{ frappe.format(amnt, {'fieldtype': 'Currency'}) }}\r\n                            </td>\r\n                            \r\n                        </tr>\r\n                    {% endif %}\r\n                {% endif %}\r\n            {% endfor %}\r\n        {% endfor %}\r\n\r\n\r\n        <tr>\r\n            <td width=\"6%\" valign=\"top\">\r\n\r\n            </td>\r\n            <td width=\"25%\" valign=\"top\">\r\n                <strong>RMC</strong>\r\n            </td>\r\n            <td width=\"8%\" valign=\"top\">\r\n\r\n            </td>\r\n            <!--            <td width=\"8%\" valign=\"top\">-->\r\n\r\n            <!--</td>-->\r\n                        <td width=\"8%\" valign=\"top\">\r\n\r\n            </td>\r\n            <td width=\"10%\" valign=\"top\">\r\n\r\n            </td>\r\n            <td width=\"18%\" valign=\"top\" align=\"right\">\r\n                <strong>{% set amnt = se.total_outgoing_value %}\r\n                 {{ frappe.format(amnt, {'fieldtype': 'Currency'}) }}\r\n                \r\n                </strong>\r\n            </td>\r\n           \r\n        </tr>\r\n        <tr>\r\n            <th colspan=\"8\" ><p align=\"center\"><strong><h5>Additional Costs</h5></strong></p></th>\r\n        </tr>\r\n        {% for row in se.additional_costs %}\r\n        {% if row.qty != 0  %} \r\n         <tr>\r\n             <td>{{ loop.index }}</td>\r\n             <td>{{ row.description or \"\" }}</td>\r\n             <td></td>\r\n             <!--<td></td>-->\r\n             <td align=\"right\">{{ row.qty or \"\" }}</td>\r\n             <td align=\"right\">{{ row.get_formatted('rate') or \"\" }}</td>\r\n             <td align=\"right\">{% set amnt = row.amount or 0 %}{{ row.get_formatted('amount') }}</td>\r\n           \r\n         </tr>\r\n \r\n         {% endif %}\r\n     {% endfor %}\r\n   \r\n     <tr>\r\n        <td width=\"6%\" valign=\"top\">\r\n\r\n        </td>\r\n        <td width=\"25%\" valign=\"top\">\r\n            <strong>Total Additional Cost</strong>\r\n        </td>\r\n        <td width=\"8%\" valign=\"top\">\r\n\r\n        </td>\r\n        <!-- <td width=\"8%\" valign=\"top\">-->\r\n\r\n        <!--</td>-->\r\n         <td width=\"8%\" valign=\"top\">\r\n\r\n        </td>\r\n        <td width=\"10%\" valign=\"top\">\r\n\r\n        </td>\r\n        <td width=\"18%\" valign=\"top\" align=\"right\">\r\n            <strong>{% set amnt = se.total_additional_costs or 0 %}{{ se.get_formatted('total_additional_costs') or \"\" }}</strong>\r\n        </td>\r\n        \r\n        \r\n    </tr>\r\n           {% for row in se.items %}\r\n        {% if row.t_warehouse %}\r\n        <tr>\r\n            <th width=\"32%\" colspan= \"2\" valign=\"top\">\r\n                <h6><strong>{{ row.item_code }}</strong></h6>\r\n                <p>Yield: {{ row.batch_yield|flt(2) }}  </p>\r\n                <p>{{ row.packing_size }}{{ row.uom }} &times; {{ row.no_of_packages }} = {{ row.qty }}{{ row.uom }}</p>\r\n            </th>\r\n            \r\n             <th width=\"8%\" valign=\"top\"> <h6 align=\"right\">{{ row.lot_no or ''}}</h6></th>\r\n        <!-- <th width=\"8%\" valign=\"top\">-->\r\n        <!--    <h6 align=\"right\">{{ row.concentration or ''}}</h6>-->\r\n        <!--</th>-->\r\n\r\n        </th>\r\n            <th width=\"8%\" valign=\"top\" align=\"right\">\r\n                <h6 align=\"right\"> {{ row.qty|flt(2) }}</h6>\r\n            </th>\r\n            <th width=\"10%\" valign=\"top\" align=\"right\">\r\n\r\n                <h6 align=\"right\">{{ frappe.format(row.valuation_rate, {'fieldtype': 'Currency'}) }}</h6>\r\n\r\n            </th>\r\n            <th width=\"16%\" valign=\"top\" align=\"right\">\r\n                {% if row.valuation_rate and row.qty %}\r\n              \r\n                <h6 align=\"right\"><strong>{{ row.get_formatted('amount') }}<strong></h6>\r\n                {% endif %}\r\n            </th>\r\n            <!--<th>-->\r\n            <!--</th>-->\r\n        </tr>\r\n        {% endif %}\r\n    {% endfor %}\r\n        {% endif %}\r\n    </tbody>\r\n</table>\r\n\r\n{% else%}\r\n<h1 align=\"center\"><b>In Process : {{ doc.production_item or \"\" }}</b></h1>\r\n<table  border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" style=\"margin-top: 20px\">\r\n\t<tr>\r\n        <th  valign=\"top\">\r\n            <strong>No</strong>\r\n        </th>\r\n        <th  valign=\"top\">\r\n            <strong>Raw Material</strong>\r\n        </th>\r\n        <th valign=\"top\">\r\n            <strong>Requried Qty</strong>\r\n        </th>\r\n        <th valign=\"top\">\r\n            <strong>Transferred Qty</strong>\r\n        </th>\r\n    </tr>\r\n\t{% for row in doc.required_items %}\r\n        <tr>\r\n            <td width=\"6%\" valign=\"top\">\r\n               {{  row.idx }}            </td>\r\n            <td width=\"25%\" valign=\"top\">\r\n                {{ row.item_code }}\r\n            </td>\r\n            <td width=\"10%\" align=\"right\" valign=\"top\">\r\n                {{ row.required_qty|flt(2) or \"-\" }}\r\n            </td>\r\n            <td width=\"10%\" valign=\"top\" align=\"right\">\r\n                {{ row.transferred_qty or \"-\" }}\r\n            </td>\r\n        </tr>\r\n    {% endfor %}\r\n</table>\r\n{% endif %}\r\n\r\n<div id=\"footer-html\" class=\"hidden-pdf\">\r\n    {%- if not no_letterhead and footer -%}\r\n    <div class=\"letter-head-footer\">\r\n        {{ footer }}\r\n    </div>\r\n    {%- else -%}\r\n    <div>\r\n        <br>\r\n        <br>\r\n        <br>\r\n        <br>\r\n        <br>\r\n        <br>\r\n        <br>\r\n        <br>\r\n    </div>\r\n    {%- endif -%}\r\n</div>",
